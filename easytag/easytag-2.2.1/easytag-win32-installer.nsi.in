  Delete /rebootok "$INSTDIR\bin\libatk-1.0-0.dll"
  Delete /rebootok "$INSTDIR\bin\iconv.dll"
  RMDir /r "$INSTDIR\usr"
  RMDir /r "$INSTDIR\share"
  RMDir /r "$INSTDIR\lib"
  RMDir /r "$INSTDIR\etc"
  Delete "$INSTDIR\${PRODUCT_NAME}.url"
